#include "main.h"
#include <limits.h>
int main(void)
{
	int len, len2;

	len = _printf("% d\n", INT_MIN);
	len2 = printf("% d\n", INT_MIN);
	printf("%d, %d\n", len, len2);
	len = _printf("%+d", 1024);
	len2 = printf("%+d", 1024);
	printf("%d, %d\n", len, len2);
	len = _printf("%#X\n", 0);
	len2 = printf("%#X\n", 0);
	printf("%d, %d\n", len, len2);
	len = _printf("%#o", 0);
	len2 = printf("%#o", 0);
	printf("%d, %d\n", len, len2);
	return (0);
}
